@model FoundersPC.Web.Domain.Common.Hardware.PowerSupply.PowerSupplyInsertDtoViewModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
@section Styles {
    <link
        href="~/css/neon-btn-styles.css"
        rel="stylesheet">
    <link
        href="~/css/entity-edit.css"
        rel="stylesheet">
}


}

<h1
    class='mt-4 text-center text-light'>
    Add Power Supply Page
</h1>
<div
    class='mt-5 row'>
<div
    class='col-12 col-md-12 col-sm-12 col-xl-12 col-xxl-12'>
<form
    asp-action='Create'
    asp-controller='Producers'
    class='d-flex flex-column justify-content-center'
    method='post'>
<div
    asp-validation-summary='ModelOnly'
    class='text-danger'>
</div>
<div
    class='d-grid'>
<div
    class='row'>
    <div
        class='form-label-group'>
        <div
            class='input-group'>
            <input
                asp-for='Power'
                class='bg-dark form-control text-light'
                placeholder='ShortName'
                value='@Model.Power'>
            <label>Power</label>
        </div>
    </div>
    <span
        asp-validation-for='Power'
        class='text-danger'>
    </span>
</div>
<div
    class='row-cols-auto'>
    <div
        class='form-label-group'>
        <div
            class='input-group'>
            <input
                asp-for='Efficiency'
                class='bg-dark form-control text-light'
                placeholder='FullName'
                value='@Model.Efficiency'>
            @Html.LabelFor(x => x.Efficiency)
        </div>
    </div>
    <span
        asp-validation-for='Efficiency'
        class='text-danger'>
    </span>
</div>
<div
    class='row'>
    <div
        class='form-label-group'>
        <div
            class='input-group'>
            <input
                asp-for='MotherboardPowering'
                class='bg-dark form-control text-light'
                placeholder='Country'
                value='@Model.MotherboardPowering'>
            <label>Motherboard Powering</label>
        </div>
    </div>
    <span
        asp-validation-for='MotherboardPowering'
        class='text-danger'>
    </span>
</div>
<div
    class='row'>
    <div
        class='form-label-group'>
        <div
            class='input-group'>
            <input
                asp-for='IsModular'
                class='bg-dark form-control text-light'
                placeholder='Website'
                value='@Model.IsModular'>
            <label>Is modular</label>
        </div>
    </div>
    <span
        asp-validation-for='IsModular'
        class='text-danger'>
    </span>
</div>
<div
    class='row'>
    <div
        class='form-label-group'>
        <div
            class='input-group'>
            <input
                asp-for='CPU4PIN'
                class='bg-dark form-control text-light'
                placeholder='FoundationDate'>
            <label>CPU 4PIN</label>
        </div>
    </div>
    <span
        asp-validation-for='CPU4PIN'
        class='text-danger'>
    </span>
</div>
<div
    class='row'>
    <div
        class='form-label-group'>
        <div
            class='input-group'>
            <input
                asp-for='CPU8PIN'
                class='bg-dark form-control text-light'
                placeholder='FoundationDate'>
            <label>CPU 8PIN</label>
        </div>
    </div>
    <span
        asp-validation-for='CPU8PIN'
        class='text-danger'>
    </span>
</div>
<div
    class='row'>
    <div
        class='form-label-group'>
        <div
            class='input-group'>
            <input
                asp-for='FanDiameter'
                class='bg-dark form-control text-light'
                placeholder='FoundationDate'>
            <label>Fan diameter</label>
        </div>
    </div>
    <span
        asp-validation-for='FanDiameter'
        class='text-danger'>
    </span>
</div>
<div
    class='row'>
    <div
        class='form-label-group'>
        <div
            class='input-group'>
            <input
                asp-for='Certificate80PLUS'
                class='bg-dark form-control text-light'
                placeholder='FoundationDate'>
            <label>Certificate 80PLUS</label>
        </div>
    </div>
    <span
        asp-validation-for='Certificate80PLUS'
        class='text-danger'>
    </span>
</div>
<div
    class='row'>
    <div
        class='form-label-group'>
        <div
            class='input-group'>
            <input
                asp-for='PFC'
                class='bg-dark form-control text-light'
                placeholder='FoundationDate'>
            <label>PFC</label>
        </div>
    </div>
    <span
        asp-validation-for='PFC'
        class='text-danger'>
    </span>
</div>
<div
    class='row'>
    <div
        class='form-label-group'>
        <div
            class='input-group'>
            <input
                asp-for='Title'
                class='bg-dark form-control text-light'
                placeholder='FoundationDate'>
            <label>Title</label>
        </div>
    </div>
    <span
        asp-validation-for='Title'
        class='text-danger'>
    </span>
</div>
<div
    class='row'>
    <div
        class='form-label-group'>
        <div
            class='input-group'>
            <input
                asp-for='ProducerId'
                class='bg-dark form-control text-light'
                placeholder='FoundationDate'>
            <label>Producer Id</label>
        </div>
    </div>
    <span
        asp-validation-for='ProducerId'
        class='text-danger'>
    </span>
</div>
<div
    class='form-group row'>
    <input
        class='btn btn-apply btn-neon neon-green'
        type='submit'
        value='Save'/>
</div>
</div>
</form>
</div>
<hr
    class='hr-runner'/>
</div>
<div
    class='align-items-center d-flex justify-content-center mt-4 text-light'>
    <a
        asp-action='ProducersTable'
        asp-controller='Producers'
        class='btn btn-apply btn-neon neon-yellow'>
        Back to Table
    </a>
</div>